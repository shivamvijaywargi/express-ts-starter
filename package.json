{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build": "rimraf ./dist && swc ./src -d dist",
    "build:start": "yarn build && yarn start:prod",
    "lint": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prepare": "husky install",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --pretty --transpile-only --rs src/server.ts",
    "start:prod": "node dist/server.js",
    "test": "jest --coverage"
  },
  "keywords": [
    "Express TypeScript Starter",
    "Backend NodeJs starter"
  ],
  "author": {
    "name": "Shivam Vijaywargi",
    "email": "vijaywargishivam@gmail.com",
    "url": "https://github.com/shivamvijaywargi"
  },
  "maintainers": [
    {
      "name": "Shivam Vijaywargi",
      "email": "vijaywargishivam@gmail.com",
      "url": "https://github.com/shivamvijaywargi"
    }
  ],
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.53",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "homepage": "https://github.com/shivamvijaywargi/express-ts-starter#readme",
  "repository": {
    "type": "git",
